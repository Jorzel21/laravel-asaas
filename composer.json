{
    "name": "beedelivery/laravel-asaas",
    "description": "Package for integration with Asaas.",
    "keywords": ["asaas", "laravel", "bank"],
    "type": "library",
    "require": {
        "php": ">=8.0",
        "ext-json": "*"
    },
    "license": "MIT",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Bee Delivery",
            "email": "suporte@beedelivery.com.br"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "BeeDelivery\\Asaas\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeeDelivery\\Asaas\\AsaasServiceProvider"
            ],
            "aliases": {
                "Asaas": "BeeDelivery\\Asaas\\Facades\\Asaas"
            }
        }
    }
}
