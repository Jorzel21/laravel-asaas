{
    "name": "jorzelalves/laravel-asaas",
    "description": "Package for integration with Asaas.",
    "keywords": ["asaas", "laravel", "bank"],
    "type": "library",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "illuminate/support": "^11.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/console": "^11.0"
    },
    "license": "MIT",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Jorzel Alves",
            "email": "jorzelalves@jorzelalves.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Asaas\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asaas\\AsaasServiceProvider"
            ],
            "aliases": {
                "Asaas": "Asaas\\Facades\\Asaas"
            }
        }
    }
}
